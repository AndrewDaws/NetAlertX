        }
      ],
      "description": [
        {
          "language_code": "en_us",
          "string": "When the plugin should run. Good options are <code>schedule</code>"
        }
      ]
    },
    {
      "function": "RUN_SCHD",
      "type": {
        "dataType": "string",
        "elements": [
          {
            "elementType": "span",
            "elementOptions": [
              {
                "cssClasses": "input-group-addon validityCheck"
              },
